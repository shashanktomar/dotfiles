[core]
  pager = delta

[interactive]
  diffFilter = delta --color-only

[delta]
  navigate = true    # use n and N to move between diff sections

[merge]
  conflictstyle = diff3

[diff]
  colorMoved = default

[user]
  name = Shashank Tomar
  email = {{ .personalEmail }}

[core]
   sshCommand = "ssh -i ~/.ssh/id_personal"
